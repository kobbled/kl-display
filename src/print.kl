PROGRAM print
%NOBUSYLAMP
%COMMENT = 'breakout TP functions'
%UNINITVARS
%NOLOCKGROUP
%NOPAUSE = COMMAND + TPENABLE + ERROR

%include display.klh
%include print.klh

ROUTINE print__numreg
  BEGIN
    display__print(display__register(reg_no, TRUE) )
  END print__numreg

ROUTINE print__posreg
  BEGIN
    display__print(display__posreg(reg_no, grp_no, TRUE) )
  END print__posreg

ROUTINE print__io
  BEGIN
    display__print(display__io(typ, reg_no, TRUE) )
  END print__io

BEGIN
END print